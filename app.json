{
  "expo": {
    "name": "game-library",
    "slug": "game-library",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash/splash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#157489"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hamiltondevelopment.gamelibrary",
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hamiltondevelopment.gamelibrary"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "82e43972-e754-4cce-84fe-94ca20fc70c8"
      }
    }
  }
}
